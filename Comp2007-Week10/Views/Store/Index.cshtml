@using COMP2007_Week10.Models

@model IEnumerable<Genre>

@{
    ViewBag.Title = "Index";
}

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count()
    genres:
</p>
<ul>
    @foreach (Genre genre in Model)
    {
        <li>@Html.ActionLink(genre.Name,"Browse", new { genre = genre.Name })</li>
    }
</ul>

